{
	"name" : "SherLock",
	"version" : "1.0",
	"description" : "Chiffrement de mot de passe",
	"browser_action" : {
		"default_popup" : "popup.html"
	},
	"content_scripts" : [
		{
			"matches" : ["<all_urls>"],
			"js" : ["js/content.js"]
		}
	],
	"permissions": [
		"storage"
	],
	"manifest_version" : 2
}
